{
  "name": "nb-email-verification",
  "version": "1.0.8",
  "description": "n8n node to provide NeverBounce email verification",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node",
    "email",
    "verification",
    "validation",
    "neverbounce",
    "email-verification"
  ],
  "license": "MIT",
  "homepage": "https://www.neverbounce.com/",
  "author": {
    "name": "NeverBounce",
    "email": "support@neverbounce.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NeverBounce/neverbounce-n8n-nodes.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "clean": "npx rimraf dist",
    "clean-build": "npm run clean &&npm run build",
    "build": "tsc && gulp build:icons && npm run copy",
    "copy": "mkdir -p dist/nodes/NbEmailVerification dist/credentials && cp -r ../../dist/nodes/NbEmailVerification/* dist/nodes/NbEmailVerification/ && cp -r ../../dist/credentials/* dist/credentials/",
    "dev": "tsc --watch"
  },
  "files": [
      "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NeverBounceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/NbEmailVerification/NbEmailVerification.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
