{
  "name": "n8n-node-monorepo",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/NeverBounce/n8n-nodes-neverbounce.git"
  },
  "workspaces": [
    "nodes/EmailVerificationHints",
    "nodes/NbEmailVerification"
  ],
  "scripts": {
    "bootstrap": "npm install",
    "build:all": "npx rimraf dist && npm run clean --workspaces && npm run build --workspaces",
    "test:all": "npm run test --workspaces",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "dependencies": {
    "jsrsasign": "^11.1.0",
    "zoominfo-api-auth-client": "^1.0.1"
  }
}
